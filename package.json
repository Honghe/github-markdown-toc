{
  "devDependencies": {
    "babili": "^0.0.12",
    "browserify": "^14.1.0",
    "eslint": "^3.17.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "material-colors": "^1.2.5",
    "material-flux": "^1.4.0",
    "npm-run-all": "^4.0.1",
    "postcss": "^5.2.14",
    "postcss-cli": "2.6.0",
    "postcss-custom-properties": "^5.0.2",
    "postcss-for": "^2.1.1",
    "postcss-import": "^9.1.0",
    "postcss-nested": "^1.0.0",
    "sveltify": "^1.0.3"
  },
  "name": "github-markdown-toc",
  "scripts": {
    "clean": "rm dist/*",
    "build:css": "postcss --use postcss-import --use postcss-for --use postcss-nested --use postcss-custom-properties --output dist/index.css src/index.css",
    "build:js:bundle": "browserify --transform sveltify src/index.js --outfile dist/index.js",
    "build:js:minify": "babili dist --out-dir dist",
    "build:js:background": "cp src/background.js dist",
    "build:js": "npm-run-all --parallel build:js:bundle build:js:background --serial build:js:minify",
    "build:meta": "cp src/manifest.json dist",
    "build": "npm-run-all clean --parallel build:*",
    "lint": "eslint --config standard --env browser --global chrome src"
  },
  "version": "0.0.6",
  "license": "CC0-1.0"
}
