{
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-register": "^6.24.1",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdom": "^11.0.0",
    "kocha": "^1.8.0",
    "npm-run-all": "^4.0.1",
    "nyc": "^11.0.3",
    "postcss-cli": "^4.1.0",
    "postcss-nested": "^2.0.2",
    "prettier": "^1.5.2",
    "rollup": "^0.45.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-butternut": "^0.1.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-svelte": "^3.0.0",
    "yazl": "^2.4.2"
  },
  "name": "github-markdown-toc",
  "scripts": {
    "build:css": "postcss src/index.css --output dist/index.css",
    "build:js:bundle": "rollup -c",
    "build:js:background": "cp src/background.js dist",
    "build:js": "run-p build:js:*",
    "build:meta": "cross-env BABEL_ENV=esmodules node --require babel-register lib/manifest.js",
    "build:image": "cp src/*.svg dist",
    "prebuild": "rm -f dist/*",
    "build": "run-p build:*",
    "prettier:fix": "prettier --single-quote --no-semi --write \"{src,lib,test,modules/*}/*.js\"",
    "eslint:fix": "eslint src lib modules --fix",
    "lint:fix": "run-s prettier:fix eslint:fix",
    "test": "cross-env BABEL_ENV=test kocha --require babel-register test/*.js modules/*/test.js",
    "precov": "nyc npm test",
    "cov": "nyc report --reporter=text-lcov > coverage.lcov",
    "postcov": "codecov",
    "release": "npm-run-all --parallel lint:fix test --serial build zip",
    "preversion": "run-p lint:fix test",
    "version": "npm run build:meta",
    "zip": "cross-env BABEL_ENV=esmodules node --require babel-register lib/zip.js"
  },
  "version": "0.7.1",
  "license": "CC0-1.0",
  "private": true,
  "repository": "otariidae/github-markdown-toc",
  "bugs": {
    "url": "https://github.com/otariidae/github-markdown-toc/issues"
  },
  "dependencies": {
    "postcss": "^6.0.8",
    "request": "^2.81.0"
  }
}
