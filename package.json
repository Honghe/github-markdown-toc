{
  "devDependencies": {
    "codecov": "^2.2.0",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdom": "^11.0.0",
    "kocha": "^1.8.0",
    "material-colors": "^1.2.5",
    "npm-run-all": "^4.0.1",
    "nyc": "^11.0.3",
    "postcss-cli": "^4.1.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-import": "^10.0.0",
    "postcss-nested": "^2.0.2",
    "prettier": "^1.5.2",
    "reify": "^0.11.24",
    "rollup": "^0.43.0",
    "rollup-plugin-butternut": "^0.1.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-svelte": "^2.0.1",
    "yazl": "^2.4.2"
  },
  "name": "github-markdown-toc",
  "scripts": {
    "clean": "rm dist/*",
    "build:css": "postcss src/index.css --output dist/index.css",
    "build:js:bundle": "rollup -c",
    "build:js:background": "cp src/background.js dist",
    "build:js": "run-p build:js:*",
    "build:meta": "node --require reify lib/manifest.js",
    "build:image": "cp src/*.svg dist",
    "build": "npm-run-all clean --parallel build:*",
    "pretty": "prettier --single-quote --no-semi --list-different \"{src,lib,test,modules/*}/*.js\"",
    "pretty:fix": "prettier --single-quote --no-semi --write \"{src,lib,test,modules/*}/*.js\"",
    "lint": "eslint src lib modules",
    "lint:fix": "eslint src lib modules --fix",
    "lint:pretty:fix": "run-s pretty:fix lint:fix",
    "test": "kocha --require reify test/*.js modules/*/test.js",
    "precov": "nyc npm test",
    "cov": "nyc report --reporter=text-lcov > coverage.lcov",
    "precodecov": "npm run cov",
    "codecov": "codecov",
    "release": "npm-run-all --parallel lint test --serial build zip",
    "preversion": "run-p lint test",
    "version": "npm run build:meta",
    "zip": "node --require reify lib/zip.js"
  },
  "version": "0.7.1",
  "license": "CC0-1.0",
  "private": true,
  "repository": "otariidae/github-markdown-toc",
  "bugs": {
    "url": "https://github.com/otariidae/github-markdown-toc/issues"
  }
}
