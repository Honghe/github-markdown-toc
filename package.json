{
  "devDependencies": {
    "babel-preset-babili": "^0.0.12",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "material-colors": "^1.2.5",
    "npm-run-all": "^4.0.1",
    "postcss": "^5.2.14",
    "postcss-cli": "^3.0.0",
    "postcss-custom-properties": "^5.0.2",
    "postcss-for": "^2.1.1",
    "postcss-import": "^9.1.0",
    "postcss-nested": "^1.0.0",
    "reify": "^0.7.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-svelte": "^1.6.1",
    "tape": "^4.6.3"
  },
  "name": "github-markdown-toc",
  "scripts": {
    "clean": "rm dist/*",
    "build:css": "postcss src/index.css --output dist/index.css",
    "build:js:bundle": "rollup -c",
    "build:js:background": "cp src/background.js dist",
    "build:js": "run-p build:js:*",
    "build:meta": "node --require reify lib/manifest.js",
    "build:image": "cp src/*.svg dist",
    "build": "npm-run-all clean --parallel build:*",
    "lint": "eslint src lib modules",
    "test": "tape --require reify test/*.js",
    "release": "npm-run-all --parallel lint test --serial build",
    "git:add:meta": "git add dist/manifest.json",
    "preversion": "run-p lint test",
    "version": "run-s build:meta git:add:meta"
  },
  "version": "0.5.8",
  "license": "CC0-1.0",
  "repository": "otariidae/github-markdown-toc",
  "bugs": {
    "url": "https://github.com/otariidae/github-markdown-toc/issues"
  }
}
