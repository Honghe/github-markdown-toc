{
 "manifest_version": 2,
 "name": "GitHub Markdown Table of Contents",
 "description": "Show a table of contents of markdown on GitHub",
 "version": "0.2.0",
 "content_scripts": [
   {
     "matches": ["https://github.com/*"],
     "exclude_matches": [
       "https://github.com/login/*",
       "https://github.com/join",
       "https://github.com/join?*",
       "https://github.com/sessions/*",
       "https://github.com/blog/*",
       "https://github.com/about/*",
       "https://github.com/features",
       "https://github.com/business",
       "https://github.com/personal",
       "https://github.com/open-source/*",
       "https://github.com/pricing",
       "https://github.com/contact/*",
       "https://github.com/explore",
       "https://github.com/settings/*",
       "https://github.com/notifications/*",
       "https://github.com/watching/*",
       "https://github.com/pulls/*",
       "https://github.com/issues/*",
       "https://github.com/integrations/*",
       "https://github.com/showcases/*",
       "https://github.com/trending/*"
     ],
     "css": ["index.css"],
     "js": ["index.js"]
   }
 ],
 "background": {
   "scripts": ["background.js"],
   "persistent": false
 },
 "permissions": [
   "https://github.com/*", "contentSettings", "tabs"
 ]
}
